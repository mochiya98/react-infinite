{
  "name": "react-infinite",
  "version": "0.13.0",
  "description": "A browser-ready efficient scrolling container based on UITableView",
  "main": "build/react-infinite.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/seatgeek/react-infinite"
  },
  "keywords": [
    "react",
    "react-component",
    "scrolling",
    "uitableview",
    "infinite"
  ],
  "author": "Gareth Tan",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/seatgeek/react-infinite/issues"
  },
  "browser": "build/react-infinite.js",
  "scripts": {
    "test": "jest",
    "lint": "npx eslint --ext .jsx --ext .js ./src ./__tests__ ./pipeline ./cypress && :",
    "fix": "npx eslint --fix --ext .jsx --ext .js ./src ./__tests__ ./pipeline ./cypress && :",
    "typecheck": "npx flow version && flow check",
    "verify": "npm test && npm run typecheck && npm run lint",
    "preversion": "npm run verify",
    "prepublish": "npx gulp release",
    "lint-staged": "npx lint-staged",
    "precommit": "npx lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.js": "npm run fix",
      "*.jsx": "npm run fix"
    }
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "roots": [
      "<rootDir>/__tests__",
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/helpers"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./config/jest.js"
    ],
    "collectCoverage": true,
    "verbose": true
  },
  "homepage": "https://github.com/seatgeek/react-infinite",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.22",
    "babelify": "10.0.0",
    "browserify": "^16.2.3",
    "coveralls": "^3.0.2",
    "create-react-class": "15.6.3",
    "del": "3.0.0",
    "envify": "4.1.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.12.0",
    "eslint-config-prettier": "3.4.0",
    "eslint-config-seatgeek-react-standard": "6.0.0",
    "eslint-config-seatgeek-standard": "11.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.3",
    "eslint-plugin-standard": "4.0.0",
    "flow-bin": "0.90.0",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0",
    "gulp-concat": "^2.4.3",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp-webserver": "^0.9.1",
    "husky": "1.3.1",
    "jest-cli": "23.6.0",
    "lint-staged": "8.1.0",
    "merge-stream": "1.0.1",
    "moment": "^2.10.6",
    "prettier": "1.15.3",
    "prop-types": "^15.5.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "16.7.0",
    "uglifyify": "5.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^2.0.0",
    "watch": "^1.0.2",
    "watchify": "^3.4.0",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "lodash.isarray": "4.0.0",
    "lodash.isfinite": "3.3.2",
    "object-assign": "4.1.1"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "prop-types": "^15.5.0"
  },
  "browserify": {
    "transform": [
      "envify"
    ]
  }
}
